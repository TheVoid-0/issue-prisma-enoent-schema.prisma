generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["interactiveTransactions"]
  output   = "./client"
  binaryTargets = ["linux-musl", "darwin", "windows"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model devices {
  id                             Int                                         @id
}
