  contato2            String?                @db.VarChar(60)
  contato3            String?                @db.VarChar(60)
  valor               String?                @db.VarChar(60)
  editable            String?                @default("true") @db.VarChar(30)
  coduni              Int?
  local               Int?
  pago                String?                @db.VarChar(40)
  modopag             String?                @db.VarChar(80)
  parcelas            Int?
  ocorrenciaRepeticao Int?                   @default(0)
  codconta            String?                @default("") @db.VarChar(10)
  medicoSolicitante   String?                @default("") @db.VarChar(50)
  codVinculo          Int?                   @default(0)
  desconto            Decimal?               @default(0.00) @db.Decimal(7, 2)
  tipoDesconto        String?                @default("") @db.VarChar(2)
  valorFinal          Decimal?               @default(0.00) @db.Decimal(7, 2)
  deuDesconto         String?                @default("") @db.VarChar(2)
  id_solicitacao      String?                @default("") @db.VarChar(30)
  usuarioAtendimento  String?                @default("") @db.VarChar(100)
  flagevento          Int?                   @default(0)
  fromApp             String?                @default("false") @db.VarChar(7)
  obsApp              String?                @db.VarChar(1000)
  coducad             Int?                   @default(0)
  datacadeve          DateTime?              @db.DateTime(0)
  vinculoApp          eventos_vinculoApp?    @default(false)
  metadados           String?                @db.Text
  flag_migracao       eventos_flag_migracao? @default(false)
  data_migracao       DateTime?              @db.DateTime(0)
  data_edicao         DateTime?              @db.DateTime(0)
  codu_update         Int?
  faturamentoentrada  faturamentoentrada?
  pacienteRelation    pacientes?             @relation(fields: [codp], references: [codp])
  usuario             usuarios?              @relation(fields: [codu], references: [codu])
  // medicoSolicitanteRelation   medicoSolicitante?     @relation(fields: [medicoSolicitante], references: [id])
  convenioRelation convenios? @relation(fields: [codconvenio], references: [codconvenio])

  @@index([codcon], map: "codcon")
  @@index([codconvenio], map: "codconvenio")
  @@index([codp], map: "codp")
  @@index([codplano], map: "codplano")
  @@index([codproc], map: "codproc")
  @@index([coducad], map: "coducad")
  @@index([coduni], map: "coduni")
  @@index([end], map: "end")
  @@index([codu, status, start], map: "eventos_dia")
  @@index([local], map: "local")
  @@index([ocorrenciaRepeticao], map: "ocorrenciaRepeticao")
}

