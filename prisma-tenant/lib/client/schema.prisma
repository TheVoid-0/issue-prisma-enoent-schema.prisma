model config_clinica {
  id          Int                        @id @default(autoincrement())
  nome_config String?                    @unique(map: "nome_config") @db.VarChar(100)
  texto       String?                    @db.MediumText
  true_false  BoolEnum?
  numerico    Int?
}

model orcamento {
  codorc         Int                  @id @default(autoincrement())
  idevento       Int?
  descricao      String?              @default("") @db.VarChar(500)
  statusorc      orcamento_statusorc? @default(elaboracao)
  codu           Int?
  codp           Int?
  datacad        DateTime?            @db.Date
  datavalid      DateTime?            @db.Date
  dataaprov      DateTime?            @db.Date
  observacao     String?              @db.VarChar(1000)
  motivorecusa   Int?
  deleted        orcamento_deleted?   @default(false)
  codcon         Int?                 @default(0)
  codu_prof      Int?
  usuario        usuarios?            @relation("usuario", fields: [codu], references: [codu])
  profissional   usuarios?            @relation("profissional", fields: [codu_prof], references: [codu])
  paciente       pacientes?           @relation(fields: [codp], references: [codp])
  itensOrcamento orcamentoitens[]
}

model orcamentoitens {
  coditem        Int                          @id @default(autoincrement())
  codorc         Int?
  descricao      String?                      @default("") @db.VarChar(500)
  codproc        Int?
  codconvenio    Int?
  valor          Decimal?                     @db.Decimal(10, 2)
  reajuste       orcamentoitens_reajuste?     @default(false)
  tiporeajuste   orcamentoitens_tiporeajuste? @default(dbgenerated("%"))
  qtdreajuste    Decimal?                     @db.Decimal(10, 2)
  quantidade     Int?                         @default(1)
  valorfinal     Decimal?                     @db.Decimal(10, 2)
  aprovado       orcamentoitens_aprovado?     @default(false)
  usuarioAprovou Int?
  orcamento      orcamento?                   @relation(fields: [codorc], references: [codorc])
  convenio       convenios?                   @relation(fields: [codconvenio], references: [codconvenio])
  procedimento   procedimentos?               @relation(fields: [codproc], references: [codproc])
}

model orcamentorecusas {
  codrecusa Int     @id @default(autoincrement())
  descricao String? @default("") @db.VarChar(200)
}

model eventos {
  id                  Int                    @id @default(autoincrement())
  title               String                 @db.VarChar(2000)
