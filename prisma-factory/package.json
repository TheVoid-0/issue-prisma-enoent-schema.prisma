{
  "name": "@thevoidtest/prisma-factory",
  "version": "1.0.0",
  "description": "Conjunto de código comum para as APIs em Nest. Possui ferramentas de autenticação e serviços de cache, por exemplo. Para adicionar a um projeto nest importe AmplimedCommonModule ao seu AppModule",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "files": [
    "dist/*.js",
    "dist/*.ts",
    "dist/**/*.js",
    "dist/**/*.ts"
  ],
  "scripts": {
    "build:dev": "yarn build && yarn copyfiles package.json ./dist",
    "build": "rimraf dist && tsc -p ./tsconfig.build.json &&  babel lib --extensions \".js,.ts\" --out-dir dist/lib --copy-files",
    "pre:publish": "yarn build && yarn publish",
    "test": "jest"
  },
  "dependencies": {
    "@thevoidtest/prisma-tenant": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@nestjs/common": "8.4.2",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "8.4.2",
    "@nestjs/typeorm": "^8.0.3",
    "@types/babel__core": "^7.1.19",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "mysql2": "^2.3.3",
    "prettier": "^2.3.2",
    "rxjs": "^7.5.5",
    "ts-jest": "^27.1.4",
    "typescript": "4.6.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "author": "dev_amplimed",
  "license": "ISC"
}
